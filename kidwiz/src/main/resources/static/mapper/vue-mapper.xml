<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kidwiz.web.DAO.VueDAO">
	<select id="boardList" resultType="Map">
		SELECT * FROM Temp
	</select>
	
	<select id='getBoard' resultType="Map">
		SELECT ROW_NUMBER() OVER(ORDER BY bno) AS no ,writer, btitle,bdate  FROM board_consulting;
	</select>
	
	<insert id="write" parameterType="Map">
		INSERT INTO Temp (name, age) VALUES (#{name}, #{age})
	</insert>
</mapper>